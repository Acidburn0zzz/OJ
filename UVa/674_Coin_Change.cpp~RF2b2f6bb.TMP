#include <stdio.h>

#define  MAX_CENTS 7489
#define NUM_COIN_TYPE 5

int cents;
int coin_type[NUM_COIN_TYPE] = { 1,5,10,25,50 };
//int dp[NUM_COIN_TYPE][MAX_CENTS];
int dp[MAX_CENTS];

int solve()
{
    for (int i = 0; i < NUM_COIN_TYPE; i++) {
        //dp[i][0] = 0;
        dp[0] = 0;
    }
    for (int i = 1; i <= MAX_CENTS; i++) {
        //dp[0][i] = 1;
        dp[i] = 1;
    }
    for (int i = 1; i < NUM_COIN_TYPE; i++) {
        for (int j = 1; j <= MAX_CENTS; j++) {
            int num = j / coin_type[i];
            dp[j] += num;
        }
    }
    return dp[MAX_CENTS];
}

int main()
{
    freopen("UVa\\674_Coin_Change.txt", "r", stdin);
    while (scanf("%d", &cents) != EOF) {
        int res = solve();
    }

    return 0;
}